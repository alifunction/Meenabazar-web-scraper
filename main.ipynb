   "cell_type": "code",
   "source": [
    "def data_extractor(category_for_web, delivery_place, category_dataset):\n",
    "    # Set up Edge options\n",
    "    edge_options = Options()\n",
    "    edge_options.page_load_strategy = 'eager'\n",
    "\n",
    "    edge_options.add_argument(\"--headless\")  # Enable headless mode\n",
    "    edge_options.add_argument(\"--disable-gpu\")\n",
    "    edge_options.add_argument(\"--no-sandbox\")\n",
    "    edge_options.add_argument(\"--window-size=1920,1080\")\n",
    "\n",
    "    # Performance-focused arguments\n",
    "    edge_options.add_argument(\"--disable-images\")\n",
    "    edge_options.add_argument(\"--disable-javascript\")\n",
    "    edge_options.add_argument(\"--disable-plugins\")\n",
    "    edge_options.add_argument(\"--disable-extensions\")\n",
    "    edge_options.add_argument(\"--media-cache-size=1\")\n",
    "    edge_options.add_argument(\"--disk-cache-size=1\")\n",
    "    edge_options.add_argument(\"--aggressive-cache-discard\")\n",
    "    edge_options.add_argument(\"--disable-blink-features=AutomationControlled\")\n",
    "    edge_options.add_argument(\"--disable-features=VizDisplayCompositor\")\n",
    "    edge_options.add_argument(\"--disable-ipc-flooding-protection\")\n",
    "    edge_options.add_argument(\"--no-first-run\")\n",
    "    edge_options.add_argument(\"--no-default-browser-check\")\n",
    "\n",
    "    # Resource blocking\n",
    "    edge_options.add_experimental_option(\"prefs\", {\n",
    "        \"profile.managed_default_content_settings.images\": 2,\n",
    "        \"profile.managed_default_content_settings.stylesheets\": 2,\n",
    "        \"profile.default_content_settings.fonts\": 2,\n",
    "    })\n",
    "\n",
    "    edge = webdriver.Edge(options=edge_options)\n",
    "    edge.get(f'https://www.meenabazaronline.com/category/{category_for_web}')\n",
    "    time.sleep(2)\n",
    "    edge.maximize_window()\n",
    "\n",
    "    input_element = edge.find_element(By.CSS_SELECTOR, \"#cdk-overlay-0 > nz-modal-container > div > div > div.ant-modal-body.ng-tns-c8-2 > app-area-box > nz-select > nz-select-top-control > nz-select-search > input\")\n",
    "    input_element.send_keys(delivery_place)\n",
    "    time.sleep(5)\n",
    "\n",
    "    input_element.send_keys(Keys.ENTER)\n",
    "    time.sleep(10)\n",
    "\n",
    "    scroll_pause = 5 # Pause between the scrolls for avoiding errors.\n",
    "\n",
    "    last_height = edge.execute_script(\"return document.body.scrollHeight\")\n",
    "\n",
    "    product_names = []\n",
    "    product_prices = []\n",
    "\n",
    "    # Creating a loop to run through the entire category's website by scrolling until the end of the webpage\n",
    "    while True:\n",
    "        edge.execute_script(\"window.scrollTo(0, document.body.scrollHeight);\")\n",
    "        time.sleep(scroll_pause)\n",
    "        new_height = edge.execute_script(\"return document.body.scrollHeight\")\n",
    "\n",
    "        if new_height == last_height:\n",
    "            print(\"âœ… Reached the bottom of the page\")\n",
    "            break\n",
    "\n",
    "        last_height = new_height\n",
    "\n",
    "\n",
    "    # Now we create a loop to collect the entire webpage's data.\n",
    "    try:\n",
    "        for n in range(1, 100000):\n",
    "            try:\n",
    "                product_title = edge.find_element(by=By.XPATH, value=f'//*[@id=\"page\"]/div/div/app-category/div[3]/div[2]/div/app-thumb[{n}]/div/a/div/div[2]/a[1]')\n",
    "                product_names.append(product_title.text)\n",
    "                # print(product_title.text)\n",
    "\n",
    "                product_price = edge.find_element(by=By.XPATH, value=f'//*[@id=\"page\"]/div/div/app-category/div[3]/div[2]/div/app-thumb[{n}]/div/a/div/div[2]/div/div[1]/span')\n",
    "                price = product_price.text\n",
    "                product_prices.append(float(price.replace('TK', '').replace(',', '')))\n",
    "                # print(product_price.text)\n",
    "\n",
    "            except NoSuchElementException:\n",
    "                product_title = edge.find_element(by=By.XPATH, value=f'//*[@id=\"page\"]/div/div/app-category/div[4]/div[2]/div/app-thumb[{n}]/div/a/div/div[2]/a[1]')\n",
    "                product_names.append(product_title.text)\n",
    "                # print(product_title.text)\n",
    "\n",
    "                product_price = edge.find_element(by=By.XPATH, value=f'//*[@id=\"page\"]/div/div/app-category/div[4]/div[2]/div/app-thumb[{n}]/div/a/div/div[2]/div/div[1]/span')\n",
    "                price = product_price.text\n",
    "                product_prices.append(float(price.replace('TK', '').replace(',', '')))\n",
    "                # print(product_price.text)\n",
    "\n",
    "    except Exception as e:\n",
    "        print(e)\n",
    "        pass\n",
    "\n",
    "    print(f\"Product Name Numbers: {len(product_names)}\")\n",
    "\n",
    "    print(f\"Product Price Numbers: {len(product_names)}\")\n",
    "\n",
    "    category_dataset['Name'] = product_names\n",
    "    category_dataset[f'Price({today})'] = product_prices\n",
    "\n",
    "    edge.quit()\n"
   ],
