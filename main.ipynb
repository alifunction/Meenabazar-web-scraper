    "def file_saver(category_for_csv_file, new_dataframe):\n",
    "    try:\n",
    "        old_dataframe = pd.read_csv(f'{category_for_csv_file}.csv')\n",
    "\n",
    "        # Setting Name as index for both tables\n",
    "        old_dataframe_indexed = old_dataframe.set_index('Name')\n",
    "        new_dataframe_indexed = new_dataframe.set_index('Name')\n",
    "\n",
    "        #Concatenating horizontally\n",
    "        final_dataframe = pd.concat([old_dataframe_indexed, new_dataframe_indexed], axis=1)\n",
    "\n",
    "        final_dataframe.reset_index(inplace=True)\n",
    "        final_dataframe.to_csv(f'{category_for_csv_file}.csv', index=False)\n",
    "\n",
    "    except FileNotFoundError:\n",
    "        print(f\"{category_for_csv_file}.csv does not exist!\")\n",
    "        new_dataframe.to_csv(f'{category_for_csv_file}.csv', index=False)"
